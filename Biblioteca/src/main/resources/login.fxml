<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.control.PasswordField?>
<VBox id="body" alignment="CENTER" prefHeight="427.0" prefWidth="695.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="LoginController">
   <HBox id="hbox" alignment="CENTER" cacheShape="false" prefHeight="160.0" prefWidth="655.0" style="-fx-spacing: 5;">
      <VBox alignment="CENTER" style="-fx-spacing: 5;">
         <TextField fx:id="cnpTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="CNP" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </TextField>
         <TextField fx:id="numeTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="Nume" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </TextField>
         <TextField fx:id="prenumeTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="Prenume" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </TextField>
         <TextField fx:id="adresaTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="Adresa" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </TextField>
         <TextField fx:id="telefonTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="Telefon" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </TextField>
         <PasswordField fx:id="parolaTextField" alignment="CENTER" prefHeight="43.0" prefWidth="176.0" promptText="Parola" style="-fx-padding: 0;">
            <font>
               <Font size="14.0" />
            </font>
         </PasswordField>
      </VBox>
      <Button fx:id="login_button" alignment="CENTER" cache="true"
              cacheHint="SPEED" contentDisplay="CENTER"
              onAction="#onLoginButtonClick" prefHeight="37.0" prefWidth="92.0"
              style="-fx-border-insets: 0;" text="Login"
              textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <Button fx:id="register_button" alignment="CENTER" cache="true"
              cacheHint="SPEED" contentDisplay="CENTER"
              onAction="#onRegisterButtonClick" prefHeight="37.0" prefWidth="92.0"
              style="-fx-border-insets: 0;" text="Register"
              textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </HBox>
   <HBox id="hbox" alignment="CENTER_LEFT" cacheShape="false" prefHeight="100.0" prefWidth="662.0" style="-fx-spacing: 5;">
      <Label fx:id="questionLabel" alignment="CENTER_RIGHT" prefHeight="45.0" prefWidth="331.0" style="-fx-background-insets: 0;" text="No account?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="331.0" style="-fx-spacing: 5;">
         <Button fx:id="registerViewButton" alignment="CENTER" cache="true" cacheHint="SPEED" contentDisplay="CENTER" onAction="#switchRegister" prefHeight="37.0" prefWidth="92.0" style="-fx-border-insets: 0;" text="Register" textAlignment="CENTER">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
         <Button fx:id="loginViewButton" alignment="CENTER" cache="true" cacheHint="SPEED" contentDisplay="CENTER" onAction="#switchLogin" prefHeight="37.0" prefWidth="92.0" style="-fx-border-insets: 0;" text="Login" textAlignment="CENTER">
            <font>
               <Font size="14.0" />
            </font>
         </Button>
      </HBox>
   </HBox>
</VBox>

